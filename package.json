{
  "name": "assert-debug",
  "version": "1.0.0",
  "description": "Lightwweight wrapper around NodeJS assert so you can set breakpoints to assertion while debugging. ",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js . test",
    "test": "NODE_ENV=test nyc --nycrc-path='.nyc' mocha test/*.js"
  },
  "keywords": [
    "node",
    "debug",
    "assert",
    "intercept",
    "breakpoint",
    "spot",
    "analyze",
    "wrapper",
    "lightweight"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "eslint": "^6.0.1"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "include": "index.js",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./reports"
  }
}
